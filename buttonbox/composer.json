    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "buttonbox"
    ],
    "support": {
        "docs": "https://github.com/supercool/buttonbox",
        "issues": "https://github.com/supercool/buttonbox/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Supercool",
            "homepage": "http://supercooldesign.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "~3.0.0-beta.7"
    },
    "autoload": {
        "psr-4": {
          "supercool\\buttonbox\\": "src/"
        }
    },
    "extra": {
        "name": "Button Box",
        "handle": "buttonbox",
        "schemaVersion": "1.0.0",
        "changelogUrl": "https://raw.githubusercontent.com/supercool/buttonbox/master/CHANGELOG.md",
        "class": "supercool\\buttonbox\\ButtonBox"
